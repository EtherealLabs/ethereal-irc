    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "copy-webpack-plugin": "^0.2.0",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "electron": "^1.4.15",
    "electron-packager": "^8.5.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
